static boolean ABcheck(String str){
		for(int i = 0; i < str.length(); i++){
			if(str.charAt(i) == 'a'){
				if(i >= 4){
					if(str.charAt(i - 4) == 'b'){
						return true;
					}
				}
				if(i <= str.length() - 5){
					if(str.charAt(i + 4) == 'b'){
						return true;
					}
				}
				
			}
		}
		return false;
	}
